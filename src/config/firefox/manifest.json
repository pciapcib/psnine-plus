{
  "name": "Psnine Plus",
  "version": "__VERSION__",
  "manifest_version": 2,
  "author": "Kamisama",
  "description": "__DESC__",
  "homepage_url": "https://github.com/pciapcib/psnine-plus",
  "icons": {
    "128": "icons/alu.png"
  },
  "permissions": [
    "tabs",
    "*://psnine.com/*",
    "*://*.psnine.com/*"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/alu.png"
    },
    "default_title": "Psnine Plus"
  },
  "content_scripts": [
    {
      "matches": [
        "*://psnine.com/*",
        "*://*.psnine.com/*"
      ],
      "js": [
        "./jquery.min.js",
        "./tooltipster.bundle.min.js",
        "./psnine-plus.js"
      ],
      "css": [
        "./tooltipster.bundle.min.css",
        "./psnine-plus.css"
      ]
    }
  ]
}
