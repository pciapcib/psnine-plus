{
  "name": "Psnine Plus",
  "version": "__VERSION__",
  "manifest_version": 2,
  "author": "Kamisama",
  "description": "__DESC__",
  "homepage_url": "https://github.com/pciapcib/psnine-plus",
  "icons": {
    "128": "icons/alu.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "*://*.psnine.com/*"
  ],
  "browser_action": {
    "default_icon": "icons/alu.png",
    "default_popup": "popup/popup.html",
    "default_title": "Psnine Plus"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.psnine.com/*"
      ],
      "js": [
        "./jquery.min.js",
        "./tooltipster.bundle.min.js",
        "./psnine-plus.js"
      ],
      "css": [
        "./tooltipster.bundle.min.css",
        "./psnine-plus.css"
      ]
    }
  ]
}
